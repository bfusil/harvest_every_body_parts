<Defs>

  <!-- Eye -->
  <ThingDef ParentName="BodyPartNaturalBase">
    <defName>Eye</defName>
    <label>eye</label>
    <description>A biological human eye. Reacts to light and allows vision.</description>
    <descriptionHyperlinks><RecipeDef>InstallNaturalEye</RecipeDef></descriptionHyperlinks>
    <statBases>
      <MarketValue>1200</MarketValue>
      <Mass>0.2</Mass>
    </statBases>
  </ThingDef>
  
  <RecipeDef ParentName="SurgeryInstallBodyPartNaturalBase">
    <defName>InstallNaturalEye</defName>
    <label>install eye</label>
    <description>Install a biological eye.</description>
    <descriptionHyperlinks><ThingDef>Eye</ThingDef></descriptionHyperlinks>
    <jobString>Installing eye.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Eye</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Eye</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Eye</li>
    </appliedOnFixedBodyParts>
  </RecipeDef>
  
